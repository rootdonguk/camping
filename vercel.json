{
  "version": 2,
  "buildCommand": "pnpm build",
  "outputDirectory": "dist",
  "functions": {
    "dist/index.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/index.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "dest": "dist/index.js",
      "methods": ["GET"]
    }
  ]
}
