{
  "version": 2,
  "buildCommand": "pnpm build",
  "functions": {
    "api/index.ts": {
      "includeFiles": "server-dist/**"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ]
}
